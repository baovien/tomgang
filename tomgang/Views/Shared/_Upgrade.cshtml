@model Upgrade

<img 
    src="@Model.imgPath" 
    id="@Model.Id" 
    class="upgradeImg img-thumbnail" 
    style="display: none" 
    draggable="false"
    data-cost="@Model.cost" 
    data-content="@Model.description" 
    rel="popover" 
    data-placement="bottom" 
    data-original-title="@Model.Id - Price: @Model.cost" 
    data-trigger="hover">
</img>



